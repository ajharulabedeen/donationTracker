CREATE TABLE `donation`.`posts` ( `ID` INT NOT NULL AUTO_INCREMENT , `user_id` VARCHAR(512) NOT NULL , `title` VARCHAR NOT NULL , `description` MEDIUMTEXT NOT NULL , `total_needed` DOUBLE NOT NULL , `total_collected` DOUBLE NOT NULL , `total_expanse` DOUBLE NOT NULL , `start_date` VARCHAR(512) NOT NULL , `end_date` VARCHAR(512) NOT NULL , `active` BOOLEAN NOT NULL , PRIMARY KEY (`ID`)) ENGINE = InnoDB;